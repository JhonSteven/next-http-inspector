{
    "name": "next-http-inspector",
    "version": "0.2.3",
    "description": "Next Http Inspector - Interceptors for Next.js with WebSocket logging and monitoring (dev-only).",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "browser": {
        "crypto": false,
        "http": false,
        "https": false,
        "ws": false,
        "fs": false,
        "path": false,
        "url": false,
        "util": false,
        "stream": false,
        "events": false,
        "net": false,
        "tls": false
    },
    "scripts": {
        "build": "webpack --config webpack.config.js",
        "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
        "dev": "tsc --watch",
        "prepublishOnly": "npm run build && npm run build:types"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "author": "Jhon Parra",
    "license": "MIT",
    "dependencies": {
        "ws": "^8.16.0"
    },
    "devDependencies": {
        "@types/ws": "^8.18.1",
        "ts-loader": "^9.5.4",
        "typescript": "^5.6.0",
        "webpack": "^5.95.0",
        "webpack-cli": "^5.1.4"
    },
    "keywords": [
        "nextjs",
        "instrumentation",
        "interceptors",
        "logger",
        "monitoring",
        "websocket",
        "debug",
        "development",
        "dev-tools"
    ],
    "peerDependencies": {
        "next": ">=13.0.0"
    },
    "peerDependenciesMeta": {
        "next": {
            "optional": false
        }
    }
}
